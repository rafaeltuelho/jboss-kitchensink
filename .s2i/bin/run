#!/bin/sh

echo -e "\n------------------------------------------------------------------------"
echo -e "\t executing a custom run script provided by the app source repo in .s2i/bin/run"
echo -e "------------------------------------------------------------------------\n"

echo -e "\n\n\n"
echo "I'll call the my custom version of openshift-launch.sh (provided by app source repo)!"
echo -e "------------------------------------------------------------------------\n"
echo -e "\n\n\n"
exec $HOME/app-scripts/openshift-launch.sh